{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "FRONTEND_URL": "https://your-frontend-domain.vercel.app",
    "CLIP_MODEL_NAME": "openai/clip-vit-base-patch32",
    "MAX_SIMILARITY_RESULTS": "20"
  }
}
